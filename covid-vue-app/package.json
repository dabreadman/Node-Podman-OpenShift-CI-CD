{
  "name": "covid-vue-app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "node ./server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest --coverage --watchAll=false || exit 0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.10.1",
    "selenium-webdriver": "*",
    "vue": "^2.6.10",
    "vue-router": "^3.5.1",
    "vuetify": "^2.4.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@vue/babel-preset-app": "^4.5.12",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.23.0",
    "eslint-plugin-vue": "^7.8.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.1",
    "supertest": "^6.1.3",
    "vue-cli-plugin-vuetify": "^2.3.1",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
